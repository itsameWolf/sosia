#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,kscan = &kscan0;
        zmk,matrix_transform = &default_transform;
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <12>;
        rows = <5>;
// | SW1  | SW6  | SW11 | SW16 | SW21 | SW26 |                 | SW1  | SW6  | SW11 | SW16 | SW21 | SW26 |
// | SW2  | SW7  | SW12 | SW17 | SW22 | SW27 |                 | SW2  | SW7  | SW12 | SW17 | SW22 | SW27 | 
// | SW3  | SW8  | SW13 | SW18 | SW23 | SW28 |                 | SW3  | SW8  | SW13 | SW18 | SW23 | SW28 | 
// | SW4  | SW9  | SW14 | SW19 | SW24 | SW29 |                 | SW4  | SW9  | SW14 | SW19 | SW24 | SW29 | 
// | SW5  | SW10 | SW15 | SW20 | SW25 | SW30 |                 | SW5  | SW10 | SW15 | SW20 | SW25 | SW30 |
        map = <
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)                 RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5) 
RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5)                 RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5) 
RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5)                 RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5) 
RC(3,0) RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(3,5)                 RC(3,0) RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(3,5)
RC(4,0) RC(4,1) RC(4,2) RC(4,3) RC(4,4) RC(4,5)                 RC(4,0) RC(4,1) RC(4,2) RC(4,3) RC(4,4) RC(4,5)  
        >;
    };

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        label = "KSCAN";

        diode-direction = "col2row";
        row-gpios
            = <&xiao_ble 10 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&xiao_ble 9  (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&xiao_ble 8  (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&xiao_ble 7  (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&xiao_ble 6  (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            ;

    };
};